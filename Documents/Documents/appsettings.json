{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "content": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "/documents/{*any}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "myDestination": {
            "Address": "http://localhost:10000/devstoreaccount1"
          }
        },
        "LoadBalancingPolicy": "PowerOfTwoChoices"
      }
    }
  }
}
